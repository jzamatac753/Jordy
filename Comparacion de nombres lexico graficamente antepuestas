Ejercicio 2
//Este programa lee y compara nombres ‚Äù
#include<iostream>
#include<string> 
using namespace std;
int main ( )
{
cout <<"Ingrese dos nombres \n";
string first;
string second;
cin>>first;// (o puede ser asi cin>>first>>second) lee dos strings
cin>>second;
if (first==second) cout<<"Ambos son iguales\n";
if (first<second)
cout<<first<<" esta lexico graficamente antes"<<second<<"\n";
if (first>second)
cout<<first<<" esta lexico graficamente despues "<<second<<"\n";
}

